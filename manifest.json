{
    "manifest_version": 2,

    "name": "Searcherplus",
    "description": "search everywhere",
    "version": "1.0",
    "permissions": [
        "tabs",
        "<all_urls>"
    ],

    "browser_action": {
        "default_title": "popup practice",      
        "default_popup": "popup.html"        
      },
    "commands": {
        "_execute_browser_action": {
        "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  }

}